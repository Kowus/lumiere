<!-- Header -->
<!--<a name="about"></a>-->
<div class="intro-header" id="about">
    <div class="container">

        <div class="row">
            <div class="col-lg-12">
                <div class="intro-message">
                    <h1 class="fadein-head">{{{title}}}</h1>
                    <h3 class="fadein-head">Motto Here</h3>
                    <hr class="intro-divider">
                    <ul class="list-inline intro-social-buttons">
                        {{> social }}
                    </ul>
                </div>
            </div>
        </div>

    </div>
    <!-- /.container -->

</div>
<!-- /.intro-header -->

<!-- Page Content -->

<div class="content-section-a" id="services">

    <div class="container">
        <div class="row">
            <div class="col-lg-5 col-sm-6">
                <hr class="section-heading-spacer">
                <div class="clearfix"></div>
                <dl class="definitions">
                    <dt>
                        <dfn class="section-heading">
                            <h2>Light</h2>
                            <span class="lead def-margin" style="font-family: times;"> /l&#652;&#618;t/ </span>
                            <span>&sdot; noun &sdot;</span>
                        </dfn>
                    </dt>
                    <dd>
                        <p><sup>1 </sup><span class="lead" id="definition" style="margin-left: 10px">The natural agent that simulates sight and makes vision possible.</span></p>
                    </dd>
                    <dd>
                        <p><sup>2 </sup><span class="lead" id="definition" style="margin-left: 10px">An expression in someone's eyes indicating a particular emotion or mood.</span></p>

                    </dd>

                </dl>
                <!--try &#x026A; or &#618; for pronounciation i-->
            </div>
            <div class="col-lg-5 col-lg-offset-2 col-sm-6 text-center definitions">
                <!--<img class="img-responsive" src="/images/ipad.png" alt="">-->
                <h1><i class="fa fa-5x fa-lightbulb-o"></i></h1>
            </div>
        </div>

    </div>
    <!-- /.container -->

</div>
<!-- /.content-section-a -->

<div class="content-section-b" id="pricing">

    <div class="container">

        <div class="row">
            <div class="col-lg-5 col-lg-offset-1 col-sm-push-6  col-sm-6">
                <hr class="section-heading-spacer">
                <div class="clearfix"></div>
                <h2 class="section-heading">Tailored Pricing<br>
                    <small>Flexible Event Suite Packages</small>
                </h2>
                <p class="lead">
                    Because of our dedication to satisfying our customers' needs and our many years of experience, we have evolved to catering the diverse ever-increasing demands of our customers. No wonder <i class="lead stats" style="font-size: 2em;">90%</i> of our customers recommend us.
                <aside class="price-zone">You may also choose a package from our <button class="btn btn-default price-zone-btn" data-toggle="modal" data-target="#custom-mades">custom packages</button> at a relatively affordable rate</aside>
                </p>
            </div>
            <div class="col-lg-5 col-sm-pull-6  col-sm-6">
                <!--<img class="img-responsive" src="/images/dog.png" alt="">-->
                {{> pricing }}
            </div>
        </div>

    </div>
    <!-- /.container -->

</div>
<!-- /.content-section-b -->

